<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycarapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyCarApp">
        <activity
            android:name=".AdvanceLevel"
            android:label="Identify the 3 Car Models ">
        </activity>
        <activity
            android:name=".IdentifyTheCarImage"
            android:label="Identify the Image of the Car Model " />
        <activity
            android:name=".Hint"
            android:label="Gues the Car Model"/>
        <activity
            android:name=".IdentifyTheCarMake"
            android:label="Identify the Car Make"/>
        <activity
            android:name=".MainActivity"
            android:label="Welcome to the Car Quiz Game">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>